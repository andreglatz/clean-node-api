{
  "name": "clean-node-api",
  "version": "2.5.2",
  "repository": "git@github.com:AndreGlatz/clean-node-api.git",
  "author": "Andre Glatz <andreglatz@outlook.com>",
  "license": "MIT",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "start": "node dist/main/server.js",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "postbuild": "copyfiles -u 1 public/**/* dist/static",
    "build:watch": "tsc -w",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "up": "yarn build && docker-compose up -d",
    "up:mongo": "yarn build && docker-compose up -d mongo",
    "down": "docker-compose down",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "yarn test --watch -c jest-unit-config.js",
    "test:integration": "yarn test --watch -c jest-integration-config.js",
    "test:staged": "yarn test --findRelatedTests",
    "test:ci": "yarn test --coverage",
    "test:coveralls": "yarn test:ci && coveralls < coverage/lcov.info"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.1.5",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.6",
    "@types/faker": "^5.5.5",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.5.18",
    "@types/node": "^15.6.1",
    "@types/supertest": "^2.0.9",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/validator": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "apollo-server-integration-testing": "^3.0.0",
    "copyfiles": "^2.3.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^5.1.0",
    "faker": "^5.5.3",
    "git-commit-msg-linter": "^3.1.0",
    "husky": "^6.0.0",
    "jest": "^27.0.3",
    "lint-staged": "^11.0.0",
    "mockdate": "^3.0.2",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.2",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "apollo-server-express": "^2.25.0",
    "bcrypt": "^5.0.1",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "graphql-iso-date": "^3.6.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongo-round": "^1.0.0",
    "mongodb": "^3.5.7",
    "nodemon": "^2.0.4",
    "swagger-ui-express": "^4.1.4",
    "validator": "^13.7.0",
    "yarn-check": "^0.0.3"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
